@using System.Web.UI.WebControls
@model List<ExamDotNetMVC.Models.UserDetail>


@{
    Layout = "/Views/Shared/_Layout.cshtml";
}



    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()
        {
            
            var chart = new Chart(width: 1000, height: 750, theme: ChartTheme.Blue)
                .AddTitle("TollPlus Online Exam Result")
                .AddSeries("Default", chartType: "Column",
                    xValue: Model, xField: "Name",
                    yValues: Model, yFields: "UMarks")
                .AddLegend(title: "User Marks out of 10", name: null)
                .SetXAxis(title: "Exam Users", min: 1, max: 10)
                .SetYAxis(title: "Marks")
                .Write();
        }
    }




@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}


